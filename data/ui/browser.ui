<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkListStore" id="title_store">
    <columns>
      <!-- column-name entry -->
      <column type="GObject"/>
    </columns>
  </object>
  <object class="GtkListStore" id="artist_store">
    <columns>
      <!-- column-name name -->
      <column type="gchararray"/>
      <!-- column-name num -->
      <column type="guint"/>
    </columns>
  </object>
  <object class="GtkTreeModelFilter" id="title_filter">
    <property name="child_model">title_store</property>
  </object>
  <object class="GtkTreeModelFilter" id="album_filter">
    <property name="child_model">album_store</property>
  </object>
  <object class="GtkListStore" id="album_store">
    <columns>
      <!-- column-name name -->
      <column type="gchararray"/>
      <!-- column-name num -->
      <column type="guint"/>
    </columns>
  </object>
  <object class="GtkVBox" id="browser_vbox">
    <property name="visible">True</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkVPaned" id="browser_vpane">
        <property name="visible">True</property>
        <property name="can_focus">True</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkHBox" id="browser_hbox1">
            <property name="visible">True</property>
            <property name="spacing">5</property>
            <child>
              <object class="GtkScrolledWindow" id="browser_sw1">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="hscrollbar_policy">automatic</property>
                <property name="vscrollbar_policy">automatic</property>
                <child>
                  <object class="GtkTreeView" id="browser_artist">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="model">artist_store</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="position">0</property>
              </packing>
            </child>
            <child>
              <object class="GtkScrolledWindow" id="browser_sw2">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="hscrollbar_policy">automatic</property>
                <property name="vscrollbar_policy">automatic</property>
                <child>
                  <object class="GtkTreeView" id="browser_album">
                    <property name="visible">True</property>
                    <property name="can_focus">True</property>
                    <property name="model">album_filter</property>
                  </object>
                </child>
              </object>
              <packing>
                <property name="position">1</property>
              </packing>
            </child>
          </object>
          <packing>
            <property name="resize">False</property>
            <property name="shrink">True</property>
          </packing>
        </child>
        <child>
          <object class="GtkScrolledWindow" id="browser_sw3">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="hscrollbar_policy">automatic</property>
            <property name="vscrollbar_policy">automatic</property>
            <child>
              <object class="GtkTreeView" id="browser_title">
                <property name="visible">True</property>
                <property name="can_focus">True</property>
                <property name="model">title_filter</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="resize">True</property>
            <property name="shrink">True</property>
          </packing>
        </child>
      </object>
      <packing>
        <property name="position">0</property>
      </packing>
    </child>
  </object>
</interface>
